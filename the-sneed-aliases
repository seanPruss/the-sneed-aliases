alias tree='tree -a -I .git'
alias b="cd .."
alias bb="cd ../.."
alias bbb="cd ../../.."
alias bbbb="cd ../../../.."
alias p="cd -"
alias pp="cd -2"
alias ppp="cd -3"
alias pppp="cd -4"
alias lock="convert ~/Pictures/HardWallpaper.jpg -resize $(xdpyinfo | grep dimensions | sed -r 's/^[^0-9]*([0-9]+x[0-9]+).*$/\1/') RGB:- | i3lock --raw $(xdpyinfo | grep dimensions | sed -r 's/^[^0-9]*([0-9]+x[0-9]+).*$/\1/'):rgb --image /dev/stdin"

alias ls="ls -A"
alias ll="ls -l"
